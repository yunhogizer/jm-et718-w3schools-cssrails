<%= form_for([@resume, @resume.work_experiences.build]) do |f| %>
  <p>
    <%= f.label :experience %><br>
    <%= f.text_field :experience %>
  </p>
  <p>
    <%= f.label :source %><br>
    <%= f.text_field :source %>
  </p>
   <p>
    <%= f.label :duration %><br>
    <%= f.text_field :duration %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>